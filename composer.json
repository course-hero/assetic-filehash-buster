{
    "name": "course-hero/assetic-filehash-buster",
    "abandoned": "coursehero/assetic-bundle",
    "description": "Adds cache busting based on the hash of all asset contents to assetic",
    "license": "Apache-2",
    "authors": [
        {
            "name": "Jason Wentworth",
            "email": "jwentworth@coursehero.com"
        }
    ],
    "require": {
        "kriswallsmith/assetic": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "satooshi/php-coveralls": "0.6.*"
    },
    "autoload": {
        "psr-0": { "CourseHero\\AsseticFilehashBuster": "src/" }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/course-hero/assetic-filehash-buster",
            "options": {
                "ssl": {
                    "verify_peer": "true"
                }
            }
        }
    ]
}
